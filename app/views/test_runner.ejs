<%- include('header') %>
  </div>
  <!-- end .col-sm-12 -->
  </div>
  </div>
  <div class="row">
    <div class="col-sm-12">
      <h3>Run Tests:</h3>
      <p>
        <div style="-webkit-border-radius:8px; -moz-border-radius:8px; border-radius:8px; border:1px solid #ccc; width:8px; height:8px; background-color:#00ff00; display: inline-block;"></div> <b> - Running Test</b></p>
      <p>
        <div style="-webkit-border-radius:8px; -moz-border-radius:8px; border-radius:8px; border:1px solid #ccc; width:8px; height:8px; background-color:#ff0000; display: inline-block;"></div> <b> - Stopped Test</b></p>
    </div>
    <div class="col-4">
    </div>
    <div class="col-4">
      <% if(scripts){ %>
        <% for(var i=0; i < scripts.length; i++) { %>
        <a href="/test-runner/<%= scripts[i] %>">
          <button type="button" class="btn btn-primary" style="width: 15%; margin:1px;">

            <b>Run Test: <%= scripts[i] %></b>
            <br>
            <% if(status[i] === "running"){%>
              <div style="-webkit-border-radius:8px; -moz-border-radius:8px; border-radius:8px; border:1px solid #ccc; width:8px; height:8px; background-color:#00ff00; display: inline-block;"></div>
              <% }else{ %>
                <div style="-webkit-border-radius:8px; -moz-border-radius:8px; border-radius:8px; border:1px solid #ccc; width:8px; height:8px; background-color:#ff0000; display: inline-block;"></div>
                <% }%>
                
          </button>
        </a>
          <% if(i%3 === 2){%>
            <br>
            <% }%>
              <%}}%>
    </div>
    <div class="col-4"></div>
  </div>
  <br>
  <br>
  </div>
  <%- include('footer') %>
