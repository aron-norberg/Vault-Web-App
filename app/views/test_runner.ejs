<%- include('header') %>
  </div>
  <!-- end .col-sm-12 -->
  </div>
  </div>
  <div class="row">
    <div class="col-sm-12">
      <h3>Run Tests:</h3>
    </div>
  </div>
  <div class="row">
    <div class="col-4">
    </div>
    <div class="col-4">
          <table class="table">
            <tr>
              <th>Scripts</th>
              <th>Status/Progress</th>
              <th>Start/Stop</th>
            </tr>
            <% for(var i=0; i < scripts.length; i++) { %>
              <tr>
              <td><a href="/test-runner/<%= scripts[i] %>"><h4><%= scripts[i] %></h4> </a></td>
              <td><%= status[i] %></td>
              <% if(status[i] === "stopped"){ %>
              <td><button class="btn btn-success">Start</button></td>
              <% }else{ %>

              <td><button class="btn btn-danger">Stop</button></td>
              <% } %>  
              </tr>
            <% } %>
          </table>
    </div>
    <div class="col-4"></div>
  </div>
  <br>
  <br>
  <br>
  <br>

  <%- include('footer') %>
